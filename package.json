{
  "name": "calendar-mcp-app",
  "version": "1.0.0",
  "description": "AI-powered Google Calendar app using MCP via Composio",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node --watch backend/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "composio-core": "^0.4.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
